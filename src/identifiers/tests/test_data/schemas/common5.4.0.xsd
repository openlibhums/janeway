						<xsd:enumeration value="msn"/>
						<xsd:enumeration value="scirus"/>
						<xsd:enumeration value="yahoo"/>
						<xsd:enumeration value="iParadigms"/>
						<xsd:enumeration value="similarity-check"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="label" use="optional">
